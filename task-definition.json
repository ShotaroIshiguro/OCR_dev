{
  "family": "my-python-app-task-test",
  "taskRoleArn": "arn:aws:iam::220749297321:role/ECSTaskSSMRole-test",
  "networkMode": "bridge",
  "containerDefinitions": [
    {
      "name": "my-python-app-container-test",
      "image": "IMAGE_URI_PLACEHOLDER",
      "cpu": 3584,
      "memory": 15360,
      "portMappings": [
        {
          "containerPort": 8000,
          "hostPort": 8000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
              "awslogs-group": "/ecs/my-python-app-task-test",
              "awslogs-region": "ap-southeast-2",
              "awslogs-stream-prefix": "ecs"
          }
      }
    }
  ],
  "requiresCompatibilities": ["EC2"]
}